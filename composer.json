    "description": "Github API facade",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "DevBoardLib\\GithubApiFacade\\": "src/",
            "spec\\DevBoardLib\\GithubApiFacade\\": "spec/",
            "tests\\DevBoardLib\\GithubApiFacade\\": "tests/"
